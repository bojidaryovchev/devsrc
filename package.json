    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cookies-next": "6.1.1",
    "gsap": "3.13.0",
    "lucide-react": "0.552.0",
    "motion": "12.23.24",
    "next": "16.0.1",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "0.4.6",
    "prisma": "6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.0",
    "react-hot-toast": "2.6.0",
    "sst": "3.9.28",
    "swiper": "12.0.3",
    "swr": "2.3.6",
    "tailwind-merge": "3.3.1",
    "uuid": "13.0.0",
